<br><br>
<h1>New User:</h1>
<%= form_with model: @user do |f| %>
  <%= f.label :username, "Your Username Here:" %> <br>
  <%= f.text_field :username, value: 'Pepe' %> <br><br>
  <%= f.label :email, "Email:" %> <br>
  <%= f.email_field :email, placeholder: 'email@email.com' %> <br><br>
  <%= f.label :password, "Password:" %> <br>
  <%= f.password_field :password %> <br><br>
  <%= f.submit %>
<% end %>
