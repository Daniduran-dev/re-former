<%# <br><br>
<form action="/users" method="post" accept-charset="UTF-8">
  <input type="hidden" name="authenticity_token" value="<%= form_authenticity_token >">
  <label for="username">Username:</label><br>
  <input type="text" id="username" name="user[username]"><br><br>
  <label for="email">Email:</label><br>
  <input type="text" id="email" name="user[email]"><br><br>
  <label for="password">Password:</label><br>
  <input type="password" id="password" name="user[password]"><br><br>
  <input type="submit" value="Create">
</form> %>

<br><br>
<%= form_with model: @user do |u| %>
  <%= u.label :username, "Your Username Here:" %> <br>
  <%= u.text_field :username, value: 'Pepe' %> <br><br>
  <%= u.label :email, "Email:" %> <br>
  <%= u.email_field :email, placeholder: 'email@email.com' %> <br><br>
  <%= u.label :password, "Password:" %> <br>
  <%= u.password_field :password %> <br><br>
  <%= u.submit %>
<% end %>

